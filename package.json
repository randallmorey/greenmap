{
  "name": "greenmap",
  "version": "0.0.0-development",
  "description": "Map arrays asynchronously in separate threads.",
  "author": "Randall Morey <randall@randallmorey.com>",
  "repository": "randallmorey/greenmap",
  "homepage": "https://github.com/randallmorey/greenmap",
  "license": "MIT",
  "main": "greenmap.js",
  "scripts": {
    "commit": "npm test && git-cz",
    "build": "browserify greenmap.js -o dist/greenmap.js -t [ babelify --presets es2015 ]",
    "build:tests": "browserify tests.js -o dist/tests.js -t [ babelify --presets es2015 ]",
    "test": "npm run build:tests && eslint *.js && testem ci -P 3",
    "test:server": "npm run build:tests && eslint *.js && testem",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "greenlet": "^0.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^15.2.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.17.0",
    "requirejs": "^2.3.5",
    "semantic-release": "^12.4.1",
    "testem": "^2.0.0",
    "travis-deploy-once": "^4.3.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
